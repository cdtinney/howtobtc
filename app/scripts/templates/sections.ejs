<div class="masthead">

  <!-- title -->
  <h3 class="text-muted">HowToBTC</h3>
  
  <!-- navbar -->
  <nav>
   
    <!-- list of sections -->
    <ul class="nav nav-justified">      
    
      <!-- create a list item for each section -->
      <% _.each(sections, function(section) { %>
        <li><a href="<%= section.get('id') %>"><%= section.get('title') %></a></li>
      <% }); %>
      
    </ul>
    
  </nav>
  
</div>

<!-- section container -->
<div class="section">
</div>

<!-- footer -->
<footer class="footer">
  <p>&copy; Colin Tinney & Cameron Ogden</p>
</footer>